{
  "name": "styledwind",
  "displayName": "Styledwind",
  "description": "Support for .sw.ts",
  "publisher": "Punyarit",
  "version": "0.0.1",
  "engines": {
    "vscode": "^1.79.0"
  },
  "main": "./out/extension.js",
  "categories": [
    "Programming Languages"
  ],
  "repository": {
    "type": "git",
    "url": "https://github.com/Punyarit/styledwind-extension.git"
  },
  "activationEvents": [
    "onCommand:extension.activate"
  ],
  "contributes": {
    "languages": [
      {
        "id": "styledwind",
        "aliases": [
          "styledwind",
          "styledwind"
        ],
        "extensions": [
          ".sw.ts"
        ],
        "configuration": "./language-configuration.json"
      }
    ],
    "grammars": [
      {
        "language": "styledwind",
        "scopeName": "source.sw.ts",
        "path": "./syntaxes/styledwind.tmLanguage.json"
      }
    ],
    "iconThemes": [
      {
        "id": "material-icons-with-styledwind",
        "label": "Material with styledwind Icons",
        "path": "./icons/material-icons.json"
      }
    ]
  },
  "devDependencies": {
    "vscode": "^1.1.37"
  }
}
